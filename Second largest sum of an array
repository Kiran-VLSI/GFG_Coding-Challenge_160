class Solution:
    def getSecondLargest(self, arr):
   n=len(arr)
   if n<2:
       return -1;
       first = second=float('-inf')
       for num in arr:
           if num > first:
           second = first
           elif num > second and num != first:
            second = first
    if second == float('-inf'):
        return -1
        else:
           return second
        
